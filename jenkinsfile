pipeline {
    agent any 
    stages{
        stage("Build"){
            steps{
                git credentialsId: 'git', url: 'https://github.com/Raksham7/koddas_war.git'
            }
        }
        stage("UNIT Testing"){
            steps{
                sh 'mvn test' 
            }
        }
        stage('Integration Testing'){
            steps{
                sh 'mvn verify -DskipUnitTests'
            }
        }
     }
}
